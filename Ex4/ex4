#include <stdio.h> 

/*
   @brief prints all path variables
   @param argc - the size of argv array
   @param argv - arguments with the file
   @param envp - enviroments variables
*/
int main(int argc, char *argv[], char * envp[]) 
{ 
    int i; 
    for (i = 0; envp[i] != NULL; i++) 
        printf("\n%s", envp[i]); 
    getchar(); 
    return 0; 
} 
